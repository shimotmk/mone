{
	"apiVersion": 3,
	"name": "mone/icons",
	"category": "mone-block-cat",
	"title": "Icons",
	"description": "Icon wrap",
	"textdomain": "mone",
	"attributes": {
		"text": {
			"type": "string",
			"source": "html",
			"selector": ".mone-styles-switcher-title"
		},
		"isToggle": {
			"type": "boolean",
			"default": false
		},
		"openDropdownOnClick": {
			"type": "boolean",
			"default": false
		},
		"templateLock": {
			"type": [ "string", "boolean" ],
			"enum": [ "all", "insert", "contentOnly", false ]
		}
	},
	"supports": {
		"align": [ "wide", "full" ],
		"color": {
			"gradients": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		},
		"spacing": {
			"blockGap": [ "horizontal", "vertical" ],
			"padding": true,
			"margin": [ "top", "bottom" ],
			"__experimentalDefaultControls": {
				"blockGap": true
			}
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"width": true
			}
		},
		"layout": {
			"allowSwitching": false,
			"allowInheriting": false,
			"default": {
				"type": "flex"
			}
		},
		"shadow": true
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
