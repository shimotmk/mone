{
	"apiVersion": 3,
	"name": "mone/embed-site-title",
	"category": "mone-block-cat",
	"title": "Embed Site Title",
	"description": "Displays the site title obtained from the URL.",
	"textdomain": "mone",
	"ancestor": [ "mone/embed" ],
	"attributes": {
		"level": {
			"type": "number",
			"default": 0
		},
		"isLink": {
			"type": "boolean",
			"default": false
		},
		"rel": {
			"type": "string",
			"attribute": "rel",
			"default": ""
		},
		"linkTarget": {
			"type": "string",
			"default": "_self"
		}
	},
	"usesContext": [ "mone/embed-url" ],
	"supports": {
		"color": {
			"gradients": true,
			"link": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true,
				"link": true
			}
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"textAlign": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true,
			"__experimentalFontStyle": true,
			"__experimentalTextTransform": true,
			"__experimentalTextDecoration": true,
			"__experimentalLetterSpacing": true,
			"__experimentalDefaultControls": {
				"fontSize": true
			}
		},
		"html": false,
		"reusable": false
	},
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css"
}
